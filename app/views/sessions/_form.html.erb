<div class="container">
  <%=form_tag login_path do%>
  <table id='form-table'>
   <tr>
      <td> Email: </td>
      <td> <%=text_field_tag :email%></td>
   </tr>
   <tr>
      <td> Password: </td>
      <td><%=password_field_tag :password%> </td>
    </tr>
   <tr>
      <td> </td>
      <td> <%=submit_tag%></td>
   </tr>
  </table>
  <%end%>
</div>
